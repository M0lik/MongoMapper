    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.8",
    "@types/node": "^12.12.17",
    "jest": "^26.2.2",
    "nodemon": "^2.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  }
